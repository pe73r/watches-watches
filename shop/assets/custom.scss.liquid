  .desk-hidden, .deskHidden, .desk-hide, .deskHide,.desktop-hidden, .desktopHidden, .desktop-hide, .desktopHide{display : none !important;}
}
.hide, .hidden{display : none !important;}

.text-center{text-align : center;}
.flex-grow{flex-grow: 100;}
.relative{position : relative;}

.align-center, .center{
  text-align: center;
}
.align-right, .right{
  text-align: right;
}
.align-left, .left{
  text-align: left;
}
.container--small {
  width:80%;
  max-width: 1000px;
  margin: auto;
}
.image-container {
  width:100%;
  img { width:100%; }
}


.gutter-section{
  padding : 0 30px;
}

.Icon-Wrapper--clickable:hover{
  .Icon--instagram, .Icon--facebook, .Icon--pinterest, .Icon--tumblr, .Icon--twitter, .Icon--vimeo, .Icon--youtube, .Icon--linkedin, .Icon--snapchat{
    fill : #176fb2;
  }
}


/*============================== FONTS ========================================*/
@font-face {
  font-family: 'phont phreaks';
  src: url('phontphreaks-webfont.woff') format('woff');
  font-weight: normal;
  font-style: normal;
}
.phreaks{font-family : 'phont phreaks' !important;}


/*============================== FOOTER ========================================*/
.Footer__Title{display: none;}

.Footer__Social .Icon:hover{
  color:#176fb2;
}
.Footer__Social .Icon{
  &:hover{
    color:#176fb2;
  }
}
.Icon-Wrapper--clickable:hover {
  color:#176fb2;
}

.footer_logo{
  max-width: 200px;
  margin-bottom: 20px;
}
.Footer__Content {
  font-size: .9em;

}

@media #{$pocket}{
  .footer_logo{
    margin: 20px auto;
    display: none;
  }

  .Footer__Content {
    display: none;

  }
}

@media #{$portable}{
  .Footer__Block--text{
    display: none;
  }
}

/*================================ Weglot ====================================*/
#weglot-switcher-1{
  padding-bottom: 28px;
  font-family: "Josefin Sans",sans-serif;
  display : none;
}
.wg-list.country-selector a:hover{
  color : unset !important;
}
@media #{$theme-desk}{
  #weglot-switcher-1{display : block;}
}
@media #{$pocket}{
  .weglot_switcher{
    margin-left: 5px;
  }
}

/*=============================================================================*/
/*=======================  SECTION TEXT IMAGE OFFSET  =========================*/
/*=============================================================================*/
#section--text-image-offset-5{
  margin-top: 50px;
}
.custom-grid__offset-image{
  width: 100%;
}

.custom-grid__content{
  color: #535353;
  padding: 20px;
}

.custom-grid__title{
  width: 100%;
  text-align: center;
  margin-bottom: 0;
}

.custom-grid__subtitle{
  margin-bottom: 0;
}

.custom-grid__pretitle{
  margin-bottom: 0;
}

.custom-grid__mail,
.custom-grid__button{
  width: 100%;
  padding: 10px 25px;
  margin-top: 10px;
}

@media #{$pocket}{
  .Button--background{
    margin-bottom: 30px;
  }
  .custom-grid__mail{
    text-align: center;
  }
}



/*===================  TEXT IMAGE OFFSET MOBILE ==============================*/


.show--pocket{
  display: none;
}

@media #{$pocket}{

  .show--pocket{
    display: block;
  }

  .hide--pocket{
    display: none;
  }

  .custom-grid__title{
    margin-top: 30px;
  }

  .custom-grid__content{
    padding: 0 20px;
  }

}

/*=============================================================================*/
/*=======================  SECTION LOGOS  =====================================*/
/*=============================================================================*/


.logos__container{
  display: flex;
  flex-wrap: inherit;
  justify-content: center;
  align-items: flex-end;
  text-align: center;
  background-color: #fff;
}

.logos-block__content{
  padding: 40px;
}

.logos-block__text{
  font-size: .9rem;
  margin-top: 15px;
}


@media #{$pocket}{
  .logos__container{
    display: block;
  }
