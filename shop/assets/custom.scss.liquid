    #section-collection-list{
      margin-top: 20px;
    }


    /*=============================================================================*/
    /*=======================     Parallax    ======================================*/
    /*=============================================================================*/

    .parallax__section{
      width: 100%;
      height: 100%;
    }

    .parallax-float-left{
      float: left;
      margin: 0 0 0 20px;
    }

    .parallax-float-right{
      float: right;
      margin: 0 20px 0 0;
    }


    .story{
      margin: 0 auto;
      overflow: auto;
      width: 90%;
    }

    .story .parallax-float-left,
    .story .parallax-float-right{
      padding: 60px 0 0 0;
      position: relative;
      width: 50%;
    }

    .parallax-image-mobile{
      display: none;
    }

    @media #{$theme-pocket}{
      .story{
        width: 100%;
      }
      .story .parallax-float-left,
      .story .parallax-float-right{
        width: 100%;
        float: unset;
        margin: 0 auto;
        padding: 0;
      }
      .display_background{
        background-image: none !important;
      }

      .parallax-image-mobile{
        display: block;
        width: 100%;
      }
    }

