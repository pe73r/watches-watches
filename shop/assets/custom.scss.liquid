    /*=======================     overlay    ======================================*/
    /*=============================================================================*/

    .Image--contrast::after{
      background-image: -webkit-gradient(linear, left bottom, left top, from(rgba(4,4,4,0.3)), to(rgba(54,54,54,0.1)));
      background-image: linear-gradient(to top, rgba(4,4,4,0.3), rgba(54,54,54,0.1));
